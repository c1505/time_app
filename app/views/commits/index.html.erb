<%= form_tag("/commits", method: "get") do %>
  <%= label_tag(:q, "Search for:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>


<% end %>

<% unless params[:q].nil? %>
  <h1><%=params[:q]%> Commit messages</h1>

    <% @commit_messages.each do |f| %>
    <ul><%= f %></ul>
    <%end%>
<%end%>
<%= @result %>here


